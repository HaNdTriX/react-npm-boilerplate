{
  "name": "react-npm-boilerplate",
  "version": "0.1.0",
  "description": "Make your personal react component and you can import use it.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib .nyc_output || rm -rf lib .nyc_output",
    "lint": "eslint src",
    "test": "ava -v",
    "test:watch": "ava -v -- --watch",
    "test:cov": "nyc npm test",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build:lib"
  },
  "ava": {
    "require": "babel-core/register",
    "files:": "test/*.spec.js",
    "babel": "inherit"
  },
  "author": "Peng Jie <dean.leehom@gmail.com> (https://github.com/neighborhood999)",
  "repository": {
    "type": "git",
    "url": "https://github.com/neighborhood999/react-npm-boilerplate.git"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^15.1.0"
  },
  "dependencies": {
    "babel-cli": "^6.8.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "peerDependencies": {
    "react": "^0.14.8 || ^15.1.0",
    "react-dom": "^0.14.8 || ^15.1.0"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "license": "MIT"
}
